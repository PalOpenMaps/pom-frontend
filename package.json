{
  "name": "pom-next",
  "version": "0.0.1",
  "homepage": "http://bothness.github.io/pom-next",
  "author": "Ahmad Barclay",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build-static": "vite build && touch build/.nojekyll",
    "build": "APP_ENV=node vite build",
    "package": "vite package",
    "preview": "vite preview",
    "deploy": "gh-pages -d build -t true",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "get-data": "node ./scripts/get-data.js",
    "get-poha": "node ./scripts/get-poha.js",
    "get-og-images": "node ./scripts/get-og-images.js",
    "make-places": "node ./scripts/make-places.js",
    "update-places": "node ./scripts/get-data.js && node ./scripts/make-places.js"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@onsvisual/svelte-maps": "^1.1.1",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.14.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "cookie": "^0.4.1",
    "d3-dsv": "^3.0.1",
    "debounce": "^1.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "^4.0.0",
    "maplibre-gl": "^2.4.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.46.0",
    "svelte-select": "^4.4.7",
    "vite": "^4.2.1"
  },
  "type": "module"
}
